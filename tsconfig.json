{
  "compilerOptions": {
    "module": "Node16",
    "esModuleInterop": true,
    "target": "es2021",
    "moduleResolution": "bundler",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "lib": ["es2021"],
  "exclude": ["out"]
}
